{
    "manifest_version": 3,
    "name": "Nyaa Links",
    "version": "1.01",
    "description": "Show a collection of all magnetic links from a nyaa.si page.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/links.js"],
          "matches": [
            "https://nyaa.si/*",
            "https://sukebei.nyaa.si/*"
          ]
        }
      ]
}